<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/g_tv_input_new_password_t"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.1457" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/g_tv_input_new_password_b"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.18" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/g_tv_input_new_password_s"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.0718" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/g_tv_input_new_password_e"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.7590" />

    <TextView
        android:id="@+id/tv_input_new_password"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:fontFamily="@font/pretendard_semibold"
        android:text="@string/input_new_password"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="@id/g_tv_input_new_password_b"
        app:layout_constraintEnd_toEndOf="@id/g_tv_input_new_password_e"
        app:layout_constraintStart_toStartOf="@id/g_tv_input_new_password_s"
        app:layout_constraintTop_toTopOf="@id/g_tv_input_new_password_t" />
</androidx.constraintlayout.widget.ConstraintLayout>