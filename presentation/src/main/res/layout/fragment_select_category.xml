            android:id="@+id/btn_feedback_button"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="@drawable/bg_feedback_false_btn"
            app:layout_constraintStart_toStartOf="@id/g_ibtn_category_idea_button_s"
            app:layout_constraintEnd_toEndOf="@id/g_ibtn_category_idea_button_e"
