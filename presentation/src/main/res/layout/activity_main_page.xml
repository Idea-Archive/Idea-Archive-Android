            <TextView
                android:id="@+id/tv_job_opening"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:fontFamily="@font/pretendard_bold"
                android:text="@string/job_opening"
                android:textColor="@color/gray"
                android:textSize="17sp"
                app:layout_constraintBottom_toBottomOf="@id/g_tab_bar_b"
                app:layout_constraintEnd_toEndOf="@id/g_tv_job_opening_post_e"
                app:layout_constraintStart_toStartOf="@id/g_tv_job_opening_post_s"
                app:layout_constraintTop_toTopOf="@id/g_tab_bar_t" />

