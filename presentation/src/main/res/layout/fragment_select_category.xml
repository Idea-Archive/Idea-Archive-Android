            android:id="@+id/btn_jobopening_button"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="@drawable/bg_jobopening_false_btn"
            app:layout_constraintStart_toStartOf="@id/g_ibtn_category_idea_button_s"
            app:layout_constraintEnd_toEndOf="@id/g_ibtn_category_idea_button_e"
            app:layout_constraintTop_toTopOf="@id/g_ibtn_jobopening_button_t"
            app:layout_constraintBottom_toBottomOf="@id/g_ibtn_jobopening_button_b" />


