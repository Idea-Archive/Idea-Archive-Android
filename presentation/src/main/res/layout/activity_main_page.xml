                app:layout_constraintBottom_toBottomOf="@id/g_tv_navi_header_name_b"
                app:layout_constraintEnd_toEndOf="@id/g_tv_navi_header_name_e"
                app:layout_constraintStart_toStartOf="@id/g_tv_navi_header_name_s"
                app:layout_constraintTop_toTopOf="@id/g_tv_navi_header_name_t" />

            <TextView
                android:id="@+id/tv_navi_header_email"
