                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.5795" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/g_tv_feedback_post_s"
                android:layout_width="wrap_content"
                android:layout_height="0.5dp"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.3462" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/g_tv_feedback_post_e"
                android:layout_width="wrap_content"
