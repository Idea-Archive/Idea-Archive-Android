                app:layout_constraintGuide_percent="0.8000" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/g_fbtn_write_post_floating_button_e"
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.9026" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/g_tv_write_post_bubble_s"
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.5590" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/g_tv_write_post_bubble_e"
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.7897" />

            <TextView
                android:id="@+id/tv_entire"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:fontFamily="@font/pretendard_bold"
                android:text="@string/entire"
                android:textColor="@color/gray"
                android:textSize="17sp"
                app:layout_constraintBottom_toBottomOf="@id/g_tab_bar_b"
                app:layout_constraintEnd_toEndOf="@id/g_tv_entire_post_e"
                app:layout_constraintStart_toStartOf="@id/g_tv_entire_post_s"
                app:layout_constraintTop_toTopOf="@id/g_tab_bar_t" />

            <TextView
                android:id="@+id/tv_idea"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:fontFamily="@font/pretendard_bold"
                android:text="@string/idea"
                android:textColor="@color/gray"
                android:textSize="17sp"
                app:layout_constraintBottom_toBottomOf="@id/g_tab_bar_b"
                app:layout_constraintEnd_toEndOf="@id/tv_idea_post_e"
                app:layout_constraintStart_toStartOf="@id/tv_idea_post_s"
                app:layout_constraintTop_toTopOf="@id/g_tab_bar_t" />

            <TextView
                android:id="@+id/tv_feedback"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:fontFamily="@font/pretendard_bold"
                android:text="@string/feedback"
                android:textColor="@color/gray"
                android:textSize="17sp"
                app:layout_constraintBottom_toBottomOf="@id/g_tab_bar_b"
                app:layout_constraintEnd_toEndOf="@id/g_tv_feedback_post_e"
