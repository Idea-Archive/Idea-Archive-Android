            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/g_fbtn_main_page_floating_button_e"
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.9282" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/g_fbtn_write_post_floating_button_s"
